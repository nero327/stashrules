name: 百度网盘
desc: 百度网盘解锁倍速
http:
  mitm:
    - 'pan.baidu.com'
  script:
    - match: https:\/\/pan\.baidu\.com\/rest\/\d\.\d\/membership\/user
      name: panbaidu
      type: response
      require-body: true
      timeout: 10
script-providers:
  panbaidu:
    url: https://raw.githubusercontent.com/NobyDa/Script/master/Surge/JS/BaiduCloud.js
    interval: 86400
